{
  "name": "@autodev/github-agent-action",
  "version": "0.1.2",
  "description": "AutoDev GitHub Agent Action - Automated issue analysis for GitHub repositories",
  "main": "dist/index.js",
  "bin": {
    "autodev-github-action": "./bin/action.js"
  },
  "module": "dist/index.esm.js",
  "type": "commonjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "bin",
    "action.yml",
    "package.json",
    ".env.example",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:clean": "pnpm run clean && pnpm run build",
    "dev": "rollup -c -w",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:local": "node scripts/test-local.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist node_modules/.cache",
    "prepublishOnly": "pnpm run build:clean"
  },
  "keywords": [
    "github",
    "actions",
    "automation",
    "issue-analysis",
    "webhook",
    "typescript"
  ],
  "author": "AutoDev authors",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@autodev/github-agent": "workspace:*",
    "@autodev/context-worker": "workspace:*",
    "@autodev/worker-core": "workspace:*",
    "@octokit/rest": "^20.1.2",
    "@octokit/webhooks": "^12.0.10",
    "@octokit/webhooks-types": "^7.3.2",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@octokit/types": "^14.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
